package HRsystem;

public class CourseCoordinator {
    private String employeeID;
    private String name;
    private int phone;
    private String email;
    
    public CourseCoordinator(String employeeID, String name, int phone, String email){
      this.employeeID = employeeID;
      this.name = name;
      this.phone = phone;
      this.email = email;
      status = false;
    }
    
    public void setStatus() {
      status = true;
    }
